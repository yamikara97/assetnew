
@model IEnumerable<VimaruAsset.Models.AssetTypes>
<table id="assettype" class="table table-striped table-bordered table-data">
    <thead>
        <tr>
            <th>STT</th>
            <th>Tên loại</th>
            <th>Mã loại</th>
            <th>Ghi chú</th>
            <th>Chỉnh sửa vào</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            if (item != null)
            {
        <tr>
            <td></td>
            <td>@item.AssetTypeName</td>
            <td>@item.AssetTypeCode</td>
            <td>@item.Detail</td>
            <td>@item.DateUpdate.ToString("dd-MM-yyyy HH:mm")</td>
            <td class="text-center">
                <button class="btn btn-primary action-btn border-0" data-toggle="ajax-modal" data-target="#order-model" data-url="@(Url.Action("Create") + "/" + item.Id)">
                    <span>Sửa</span>
                </button>
                <button class="btn btn-danger action-btn border-0" data-toggle="ajax-modal" data-target="#delete-model" data-url="@(Url.Action("Delete") + "/" + item.Id)">
                    <span>Xóa</span>
                </button>
            </td>

        </tr>
            }
        }

    </tbody>
</table>
<noscript></noscript>